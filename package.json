{
  "type": "module",
  "name": "fast-point-in-poly-ts",
  "version": "1.1.1",
  "description": "Quickly find points in large sets of polygons",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:test": "tsc --build tsconfig.eslint.json && esbuild index.test.ts --bundle --outfile=index.test.js --platform=node --format=esm --packages=external",
    "test": "tsc --build tsconfig.eslint.json && esbuild index.test.ts --bundle --outfile=index.test.js --platform=node --format=esm --packages=external && node --test index.test.js",
    "build": "tsc && esbuild index.ts --bundle --outfile=index.js --platform=node --format=esm --packages=external --minify",
    "lint": "prettier --check . && eslint",
    "format": "prettier --write .",
    "prepack": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "geojson",
    "point",
    "polygon"
  ],
  "author": "fabrykowski",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node-lts": "^22.0.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.17.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "7.2.0",
    "@turf/center-of-mass": "7.2.0",
    "@types/geojson": "^7946.0.16",
    "geokdbush": "^2.0.1",
    "kdbush": "^4.0.2"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabrykowski/fast-point-in-poly-ts.git"
  },
  "bugs": {
    "url": "https://github.com/fabrykowski/fast-point-in-poly-ts/issues"
  },
  "homepage": "https://github.com/fabrykowski/fast-point-in-poly-ts#readme",
  "overrides": {
    "typescript-eslint": {
      "typescript": "$typescript"
    }
  }
}
